{
  "extends": [
    "@rocketseat/eslint-config/node",
    "@rocketseat/eslint-config/react"
  ],
  "rules": {
    "no-useless-constructor": "off",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": false,
        "endOfLine": "auto",
        "trailingComma": "es5"
      }
    ]
  }
}